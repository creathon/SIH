<%@ taglib prefix="form" uri="http://www.springframework.org/tags/form"%>


							<div class="form-group form-float">
							<label class="form-label">Location Image According To Season</label>
								<div  class="form-line">
								<form:select path="location.locationPhotos[${i}].tag" class="form-control show-tick">
                                        <option value="">-- Location Image According To Season --</option>
                                        <option value="Spring">Spring</option>
                                        <option value="Summer">Summer</option>
                                        <option value="Rainy">Rainy</option>
                                        <option value="Autumn">Autumn</option>
                                        <option value="Winter">Winter</option>
                                    </form:select>
								<form:input type="text" class="form-control" placeholder="Tag" path="location.locationPhotos[${i}].weather" name="location_weather" required="true"/>
								<form:input type="file" class="form-control" path="location.locationPhotos[${i}].file" name="location_multiple_img" required="true"/>
								
									
								</div>
							
							</div>
							 